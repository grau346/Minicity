    local function limparTodosProcessos()
        menuAtivo = false
        
        -- Desativar todos os sistemas
        autoFarmPecasAtivo = false
        autoFarmGasAtivo = false
        aimbotAtivo = false
        protecaoVidaAtiva = false
        autoCL = false
        antiQuedaAtivo = false
        revistarAtivo = false
        teleportAtivo = false
        nitroAtivado = false
        flyAtivado = false
        flyMobileAtivado = false
        wallCheckAtivado = false
        hitboxAtivado = false
        LockPickAtivado = false
        PuxarArmaSafe = false
        jogarCarroLongeAtivo = false
        espAtivo = false
        flyVeiculoAtivo = false
        
        -- Desativar Aimbot
        toggleAimbot(false)
        
        -- Desativar Auto Revistar
        toggleAutoRevistar(false)
        
        -- Desativar Fly Ve√≠culo
        toggleFlyVeiculo(false)
        
        -- Desconectar conex√µes
        if antiQuedaCon then
            antiQuedaCon:Disconnect()
            antiQuedaCon = nil
        end
        
        if noclipConexao then
            noclipConexao:Disconnect()
            noclipConexao = nil
        end
        
        if nitroConnection then
            if isMobile then
                if nitroConnection:IsA("TextButton") then
                    nitroConnection:Destroy()
                end
            else
                nitroConnection:Disconnect()
            end
            nitroConnection = nil
        end
        
        -- Parar coroutines
        if autoFarmMineracaoCoroutine then
            coroutine.close(autoFarmMineracaoCoroutine)
            autoFarmMineracaoCoroutine = nil
        end
        
        if jogarCarroLongeCoroutine then
            coroutine.close(jogarCarroLongeCoroutine)
            jogarCarroLongeCoroutine = nil
        end
        
        if autoRevistarCoroutine then
            coroutine.close(autoRevistarCoroutine)
            autoRevistarCoroutine = nil
        end
        
        if flyVeiculoCoroutine then
            coroutine.close(flyVeiculoCoroutine)
            flyVeiculoCoroutine = nil
        end
        
        -- Restaurar colis√£o do personagem
        if Player.Character then
            for _, part in ipairs(Player.Character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = true
                end
            end
        end
        
        -- Restaurar c√¢mera
        workspace.CurrentCamera.CameraType = Enum.CameraType.Custom
        local char = Player.Character
        if char then
            workspace.CurrentCamera.CameraSubject = char:FindFirstChildOfClass("Humanoid")
        end
    end

    -- ======================
    -- ‚öîÔ∏è SISTEMA DE HITBOX
    -- ======================
    local function aplicarHitbox()
        if not menuAtivo then return end
        for _, plr in pairs(Players:GetPlayers()) do
            if plr ~= Player and plr.Character then
                local char = plr.Character
                local hrp = char:FindFirstChild("HumanoidRootPart")

                if hrp then
                    if hitboxAtivado then
                        hrp.Size = hitboxSize
                        hrp.Transparency = 0.7
                        hrp.Material = Enum.Material.Neon
                        hrp.Color = Color3.fromRGB(255, 0, 0)
                        hrp.CanCollide = false
                    else
                        hrp.Size = Vector3.new(2, 2, 1)
                        hrp.Transparency = 1
                        hrp.Material = Enum.Material.Plastic
                        hrp.Color = Color3.fromRGB(255, 255, 255)
                        hrp.CanCollide = true
                    end
                end
            end
        end
    end

    local function toggleHitbox(ativado)
        if not menuAtivo then return end
        hitboxAtivado = ativado
        aplicarHitbox()
        notificar("Hitbox", hitboxAtivado and "Hitbox ativado!" or "Hitbox desativado!", 2)
    end

