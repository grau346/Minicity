    -- üéØ SISTEMA AIMBOT AVAN√áADO
    -- ======================
    local function getTargetPartFromCharacter(char, partName)
        if not char then return nil end
        local candidate
        if partName == "Head" then
            candidate = char:FindFirstChild("Head") or char:FindFirstChild("head")
        else
            candidate = char:FindFirstChild("Torso") or char:FindFirstChild("UpperTorso") or char:FindFirstChild("LowerTorso") or char:FindFirstChild("HumanoidRootPart")
        end
        return candidate
    end

    local function getClosestTarget()
        local closestPlayer, closestPart, closestDistance = nil, nil, math.huge
        local cam = workspace.CurrentCamera
        if not cam then return nil end
        
        local center = Vector2.new(cam.ViewportSize.X/2, cam.ViewportSize.Y/2)
        
        for _, player in pairs(Players:GetPlayers()) do
            if player ~= Player and player.Character and player.Character:FindFirstChild("Humanoid") and player.Character.Humanoid.Health > 0 then
                -- Team check
                if aimbotConfig.TeamCheck and player.Team == Player.Team then
                    continue
                end
                
                local part = getTargetPartFromCharacter(player.Character, "Head")
                if part and part.Parent then
                    local worldPos = part.Position
                    local screenPos, onScreen = cam:WorldToViewportPoint(worldPos)
                    local screenX, screenY, depth = screenPos.X, screenPos.Y, screenPos.Z
                    
                    if depth > 0 then
                        -- Wall check
                        if aimbotConfig.WallCheck then
                            local raycastParams = RaycastParams.new()
                            raycastParams.FilterDescendantsInstances = {Player.Character}
                            raycastParams.FilterType = Enum.RaycastFilterType.Blacklist
                            raycastParams.IgnoreWater = true
                            
                            local raycastResult = workspace:Raycast(cam.CFrame.Position, (worldPos - cam.CFrame.Position), raycastParams)
                            if raycastResult and raycastResult.Instance and not raycastResult.Instance:IsDescendantOf(player.Character) then
                                continue
                            end
                        end
                        
                        local distance = (Vector2.new(screenX, screenY) - center).Magnitude
                        if distance < closestDistance and distance <= aimbotConfig.FOV then
                            closestDistance = distance
                            closestPlayer = player
                            closestPart = part
                        end
                    end
                end
            end
        end
        
        return closestPlayer, closestPart
    end

    local function iniciarAimbot()
        if not menuAtivo then return end
        
        local cam = workspace.CurrentCamera
        local lastTick = tick()
        
        RunService:BindToRenderStep("AimbotStep", Enum.RenderPriority.Camera.Value + 1, function()
            if not aimbotConfig.Enabled then return end
            
            local currentTime = tick()
            local deltaTime = math.clamp(currentTime - lastTick, 0, 0.1)
            lastTick = currentTime
            
            local player, part = getClosestTarget()
            if player and part and part.Parent then
                local targetPosition = part.Position
                local currentCFrame = cam.CFrame
                local desiredCFrame = CFrame.new(currentCFrame.Position, targetPosition)
                local alpha = math.clamp(1 - math.exp(-aimbotConfig.Suavidade * deltaTime), 0, 1)
                cam.CFrame = currentCFrame:Lerp(desiredCFrame, alpha)
            end
        end)
        
        notificar("Aimbot", "Aimbot ativado atrav√©s do Discord!", 3)
    end

    local function toggleAimbot(ativado)
        aimbotConfig.Enabled = ativado
        aimbotAtivo = ativado
        
        if ativado then
            iniciarAimbot()
            notificar("Aimbot", "Aimbot ativado!", 2)
        else
            -- Desconectar o RenderStep quando desativado
            for _, connection in pairs(getconnections(RunService.RenderStepped)) do
                if connection.Function and debug.info(connection.Function, "n") == "AimbotStep" then
                    connection:Disable()
                end
            end
            notificar("Aimbot", "Aimbot desativado!", 2)
        end
    end

