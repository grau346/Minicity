    local function toggleAutoFarmMineracao()
        autoFarmMineracao = not autoFarmMineracao
        
        if autoFarmMineracao then
            if autoFarmMineracaoCoroutine then
                coroutine.close(autoFarmMineracaoCoroutine)
            end
            
            autoFarmMineracaoCoroutine = coroutine.create(function()
                while autoFarmMineracao and menuAtivo do
                    local success, err = pcall(function()
                        local char = Player.Character
                        if char and char:FindFirstChild("HumanoidRootPart") then
                            char.HumanoidRootPart.CFrame = CFrame.new(201.93, 2.76, 145.50)
                            ReplicatedStorage:WaitForChild("RemoteNovos"):WaitForChild("bixobrabo"):FireServer("/trabalhar mineracao")
                            
                            local nearestRock = nil
                            local minDistance = math.huge
                            
                            for _, obj in ipairs(workspace:GetDescendants()) do
                                if obj.Name == "Rock" and obj:IsA("BasePart") and obj.CanCollide then
                                    local distance = (char.HumanoidRootPart.Position - obj.Position).Magnitude
                                    if distance < minDistance then
                                        minDistance = distance
                                        nearestRock = obj
                                    end
                                end
                            end
                            
                            if nearestRock then
                                char.HumanoidRootPart.CFrame = CFrame.new(nearestRock.Position + Vector3.new(0, 3, 0))
                                
                                for _, child in ipairs(nearestRock:GetChildren()) do
                                    if child:IsA("ProximityPrompt") then
                                        fireproximityprompt(child)
                                        break
                                    end
                                end
                            end
                        end
                    end)
                    
                    if not success then
                        warn("‚ùå Erro no farm minera√ß√£o:", err)
                    end
                    
                    task.wait(0.5)
                end
            end)
            
            coroutine.resume(autoFarmMineracaoCoroutine)
            notificar("Auto Farm Minera√ß√£o", "‚úÖ Ativado!", 2)
        else
            if autoFarmMineracaoCoroutine then
                coroutine.close(autoFarmMineracaoCoroutine)
                autoFarmMineracaoCoroutine = nil
            end
            notificar("Auto Farm Minera√ß√£o", "‚ùå Desativado!", 2)
        end
    end

