    local function setCollisions(collide)
        if Player.Character then
            for _, part in ipairs(Player.Character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = collide
                end
            end
        end
    end

    local function ativarNoclip()
        if not menuAtivo then return end
        if noclipConexao then return end
        noclipConexao = RunService.RenderStepped:Connect(function()
            setCollisions(false)
        end)
        notificar("Noclip", "Noclip ativado!", 3)
    end

    local function desativarNoclip()
        if noclipConexao then
            noclipConexao:Disconnect()
            noclipConexao = nil
            setCollisions(true)
        end
        notificar("Noclip", "Noclip desativado!", 3)
    end

