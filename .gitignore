    local function aplicarAntiQueda()
        if not menuAtivo then return end
        local RunService = game:GetService("RunService")
        local player = game.Players.LocalPlayer

        local function applyFallControl()  
            local char = player.Character or player.CharacterAdded:Wait()  
            local hrp = char:WaitForChild("HumanoidRootPart")  
            antiQuedaCon = RunService.Heartbeat:Connect(function()  
                if antiQuedaAtivo and hrp and hrp.AssemblyLinearVelocity.Y < -75 then  
                    local vel = hrp.AssemblyLinearVelocity  
                    hrp.AssemblyLinearVelocity = Vector3.new(vel.X, -75, vel.Z)  
                end  
            end)  
        end  
        
        player.CharacterAdded:Connect(function()  
            task.wait(1)  
