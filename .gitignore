    local function getCurrentVehicleSeat()
        local character = Players.LocalPlayer.Character
        if not character or not character:FindFirstChild("Humanoid") then
            return nil
        end
        
        local humanoid = character.Humanoid
        if humanoid.SeatPart and humanoid.SeatPart:IsA("VehicleSeat") then
            return humanoid.SeatPart
        end
        return nil
    end

    local function ativarNitro()
        if not menuAtivo then return end
        local seat = getCurrentVehicleSeat()
        if not seat then
            notificar("Nitro", "Entre em um ve√≠culo primeiro!", 3)
            return
        end
        
        local success = pcall(function()
            local bodyVel = Instance.new("BodyVelocity")
            bodyVel.MaxForce = Vector3.new(math.huge, 0, math.huge)
            bodyVel.Velocity = seat.CFrame.LookVector * forcaNitro
            bodyVel.Parent = seat
            
            game:GetService("Debris"):AddItem(bodyVel, 0.3)
        end)
        
        if success then
            notificar("Nitro", "Nitro ativado!", 2)
        else
            notificar("Nitro", "Erro ao ativar nitro!", 2)
        end
    end

    local function toggleNitroAutomatico(ativado)
        if not menuAtivo then return end
        nitroAtivado = ativado
        
        if nitroAtivado then
            if nitroConnection then
                nitroConnection:Disconnect()
            end
            
            if isMobile then
                local nitroButton = Instance.new("TextButton")
                nitroButton.Name = "NitroButtonMobile"
                nitroButton.Parent = Player.PlayerGui
                nitroButton.Size = UDim2.new(0, 80, 0, 80)
                nitroButton.Position = UDim2.new(1, -100, 1, -100)
                nitroButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
                nitroButton.Text = "NITRO"
                nitroButton.TextColor3 = Color3.fromRGB(255, 255, 255)
                nitroButton.TextSize = 16
                nitroButton.ZIndex = 1000
                
                local corner = Instance.new("UICorner")
                corner.CornerRadius = UDim.new(0, 20)
                corner.Parent = nitroButton
                
                nitroButton.MouseButton1Click:Connect(function()
                    ativarNitro()
                end)
                
                nitroConnection = nitroButton
            else
                nitroConnection = UIS.InputBegan:Connect(function(input, gameProcessed)
                    if gameProcessed then return end
                    
                    if input.KeyCode == Enum.KeyCode.N then
                        ativarNitro()
                    end
                end)
            end
            
            notificar("Nitro", "Nitro autom√°tico ativado!", 3)
        else
            if nitroConnection then
                if isMobile then
                    if nitroConnection:IsA("TextButton") then
                        nitroConnection:Destroy()
                    end
                else
                    nitroConnection:Disconnect()
                end
                nitroConnection = nil
            end
            notificar("Nitro", "Nitro autom√°tico desativado!", 3)
        end
    end

