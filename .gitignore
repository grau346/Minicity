Instance.new("UICorner", toggleBtn).CornerRadius = UDim.new(0,8)
local sliderBar = Instance.new("Frame", frame)
sliderBar.Size = UDim2.new(0,160,0,10)
sliderBar.Position = UDim2.new(0,70,0,84)
sliderBar.BackgroundColor3 = Color3.fromRGB(155,175,255)
Instance.new("UICorner", sliderBar).CornerRadius = UDim.new(0,5)
local knob = Instance.new("ImageButton", frame)
knob.Size = UDim2.fromOffset(18,18)
knob.Position = UDim2.new(0,70,0,80)
knob.Image = "rbxassetid://3570695787"
knob.BackgroundTransparency = 1
knob.ImageColor3 = Color3.fromRGB(255,255,255)
local speedLabel = Instance.new("TextLabel", frame)
speedLabel.Size = UDim2.new(0,60,0,18)
speedLabel.Position = UDim2.new(0,232,0,77)
speedLabel.BackgroundTransparency = 1
speedLabel.Font = Enum.Font.Gotham
speedLabel.TextColor3 = Color3.fromRGB(255,255,255)
speedLabel.TextSize = 15
local flying = false
local flySpeed = 25
local minSpeed, maxSpeed = 1, 400
local move = {f=0,b=0,l=0,r=0,u=0,d=0}
local inputConns = {}
local flyLoop, dieConn
local function setSpeedUI(val)
    flySpeed = math.clamp(val, minSpeed, maxSpeed)
    local relX = (flySpeed-minSpeed)/(maxSpeed-minSpeed)
    local len = math.max(sliderBar.AbsoluteSize.X - knob.AbsoluteSize.X, 1)
    local x = math.floor(relX * len)
    knob.Position = UDim2.new(0, 70 + x, 0, 80)
    speedLabel.Text = ("Vel:%d"):format(flySpeed)
end
local dragging = false
knob.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then dragging = true end
end)
UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then dragging = false end
end)
RunService.RenderStepped:Connect(function()
    if dragging then
        local mx = UserInputService:GetMouseLocation().X
        local abs = sliderBar.AbsolutePosition.X
        local len = math.max(sliderBar.AbsoluteSize.X - knob.AbsoluteSize.X, 1)
        local rel = math.clamp(mx - abs, 0, len)
        knob.Position = UDim2.new(0,70+rel,0,80)
        local percent = rel / len
        setSpeedUI( math.floor(minSpeed + percent*(maxSpeed-minSpeed)) )
    end
end)
setSpeedUI(flySpeed)
local function clearInputConns()
    for _,c in pairs(inputConns) do
        if c and c.Disconnect then pcall(c.Disconnect, c) end
    end
    inputConns = {}
end
local function stopFly()
    flying = false
    toggleBtn.Text = "Ativar FLY"
    if flyLoop then pcall(function() flyLoop:Disconnect() end) end
    if dieConn then pcall(function() dieConn:Disconnect() end) end
    clearInputConns()
    local char = player.Character
    if char and char:FindFirstChild("HumanoidRootPart") then
        local root = char.HumanoidRootPart
        for _,obj in pairs(root:GetChildren()) do
            if obj:IsA("BodyVelocity") or obj:IsA("BodyGyro") or obj:IsA("AlignVelocity") then
                pcall(function() obj:Destroy() end)
            end
        end
        pcall(function() root.AssemblyLinearVelocity = Vector3.new() end)
    end
end
local function startFly()
    stopFly()
    flying = true
    toggleBtn.Text = "Desativar FLY"
    local char = player.Character or player.CharacterAdded:Wait()
    local root = char:WaitForChild("HumanoidRootPart", 5)
    local hum = char:FindFirstChildWhichIsA("Humanoid")
    if not root or not hum then return stopFly() end
    inputConns.keyDown = UserInputService.InputBegan:Connect(function(input, g)
        if g then return end
        if input.KeyCode == Enum.KeyCode.W then move.f = 1 end
        if input.KeyCode == Enum.KeyCode.S then move.b = 1 end
        if input.KeyCode == Enum.KeyCode.A then move.l = 1 end
        if input.KeyCode == Enum.KeyCode.D then move.r = 1 end
        if input.KeyCode == Enum.KeyCode.Space then move.u = 1 end
        if input.KeyCode == Enum.KeyCode.LeftControl then move.d = 1 end
    end)
    inputConns.keyUp = UserInputService.InputEnded:Connect(function(input, g)
        if g then return end
        if input.KeyCode == Enum.KeyCode.W then move.f = 0 end
        if input.KeyCode == Enum.KeyCode.S then move.b = 0 end
        if input.KeyCode == Enum.KeyCode.A then move.l = 0 end
        if input.KeyCode == Enum.KeyCode.D then move.r = 0 end
        if input.KeyCode == Enum.KeyCode.Space then move.u = 0 end
        if input.KeyCode == Enum.KeyCode.LeftControl then move.d = 0 end
    end)
    flyLoop = RunService.RenderStepped:Connect(function()
        if not flying or not root or not hum or hum.Health <= 0 then return end
        local cam = workspace.CurrentCamera
        local dirLocal = Vector3.new(move.r - move.l, move.u - move.d, move.b - move.f)
        if dirLocal.Magnitude > 0 then
            root.AssemblyLinearVelocity = cam.CFrame:VectorToWorldSpace(dirLocal.Unit * flySpeed)
        else
            root.AssemblyLinearVelocity = Vector3.new()
        end
    end)
    dieConn = hum.Died:Connect(function()
        stopFly()
    end)
end
toggleBtn.MouseButton1Click:Connect(function()
    if flying then stopFly() else startFly() end
end)
player.CharacterAdded:Connect(function()
    if flying then
        wait(0.8)
        startFly()
    end
end)
        notificar("Fly GUI", "Interface Fly carregada! Use os bot√µes para controlar.", 3)
    end

