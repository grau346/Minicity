    local function toggleFlyVeiculo(ativado)
        flyVeiculoAtivo = ativado
        
        if ativado then
            if flyVeiculoCoroutine then
                coroutine.close(flyVeiculoCoroutine)
            end
            
            flyVeiculoCoroutine = coroutine.create(function()
                local RunService = game:GetService("RunService")
                local Players = game:GetService("Players")
                local UserInputService = game:GetService("UserInputService")
                local TweenService = game:GetService("TweenService")

                local LocalPlayer = Players.LocalPlayer
                local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
                local Humanoid = Character:WaitForChild("Humanoid")

                -- Vari√°veis
                local flyEnabled = false
                local currentVehicle, bodyGyro, bodyVelocity
                local flySpeed = 50
                local moveDirection = Vector3.zero
                local moveY = 0

                -- Fun√ß√£o pra detectar ve√≠culo
                local function getVehicle()
                    if Character and Character:FindFirstChildOfClass("Humanoid") then
                        local seat = Character.Humanoid.SeatPart
                        if seat and seat:IsA("VehicleSeat") then
                            return seat.Parent
                        end
                    end
                    return nil
                end

                -- Liga/desliga o fly
                local function toggleFly(state)
                    flyEnabled = state
                    currentVehicle = getVehicle()

                    if flyEnabled and currentVehicle then
                        local root = currentVehicle:FindFirstChild("PrimaryPart") or currentVehicle:FindFirstChildWhichIsA("BasePart")
                        if not root then 
                            flyEnabled = false
                            return 
                        end

                        -- Remove antigos se existirem
                        if bodyGyro then bodyGyro:Destroy() end
                        if bodyVelocity then bodyVelocity:Destroy() end

                        bodyGyro = Instance.new("BodyGyro")
                        bodyGyro.D = 500
                        bodyGyro.P = 10000
                        bodyGyro.MaxTorque = Vector3.new(40000, 40000, 40000)
                        bodyGyro.CFrame = root.CFrame
                        bodyGyro.Parent = root

                        bodyVelocity = Instance.new("BodyVelocity")
                        bodyVelocity.MaxForce = Vector3.new(40000, 40000, 40000)
                        bodyVelocity.Velocity = Vector3.zero
                        bodyVelocity.Parent = root
                        
                        print("Fly ON - Ve√≠culo: " .. currentVehicle.Name)
                    else
                        if bodyGyro then 
                            bodyGyro:Destroy() 
                            bodyGyro = nil 
                        end
                        if bodyVelocity then 
